function raf_create_vcard(){
    return utf8_encode('BEGIN:VCARD
        VERSION:4.0
        N:;'.$name.';;;
        FN:'.$name.'
        EMAIL:'.$email.'
        ORG:'.$name.'
        TEL:'.$tel.'
        TEL;type=FAX:'.$fax.'
        URL;type=PREF:'.$www.'
        ADR:;'.$address.';
        END:VCARD');
}